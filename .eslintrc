{
  "parser": "babel-eslint",
  "extends": "airbnb/base",

  "plugins": [
    "import"
  ],

  "rules": {
    comma-dangle: [0, "never"],
    import/no-unresolved: [2],
    import/no-commonjs: [2],
    no-console: [0],
    semi: [2, "never"],
    space-before-function-paren: [2, "never"],
    space-return-throw-case: [0],
    object-shorthand: [0],
    complexity: [0],
    max-len: [2, 180, 4],
    no-unused-vars: [0],
    arrow-body-style: [0]
  },
  "settings": {
    import/parser: "babel-eslint"
  }
}
